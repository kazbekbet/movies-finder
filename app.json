{
  "expo": {
    "name": "Поиск фильмов",
    "slug": "movies-finder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app_icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.kazbekbet.moviesfinder"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "description": "Приложение на React Native для поиска фильмов."
  },
  "hooks": {
    "postPublish": [
      {
        "file": "sentry-expo/upload-sourcemaps",
        "config": {
          "organization": "Kazbek Beteev",
          "project": "movies-finder",
          "authToken": "33d0c320a7ea46e8b0df02a24ef179275fd923e1a05949c8a17fbe98866b5d61"
        }
      }
    ]
  }
}
