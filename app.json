{
  "expo": {
    "name": "Поиск фильмов",
    "slug": "movies-finder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.kazbekbet.moviesfinder"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "description": "Приложение на React Native для поиска фильмов."
  }
}
